{
  "expo": {
    "name": "Intentionality",
    "slug": "Intentionality",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./images/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./images/adaptive-icon.png",
        "backgroundColor": "#000000"
      }
    },
    "web": {
      "favicon": "./images/favicon.png"
    },
    "plugins": ["expo-font"]
  }
}
